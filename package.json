{
  "name": "muzat-react",
  "version": "1.0.0",
  "private": true,
  "description": "muzat信息平台",
  "templateInfo": {
    "name": "taro-hooks",
    "typescript": true,
    "css": "sass"
  },
  "scripts": {
    "build:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:jd": "taro build --type jd",
    "build:quickapp": "taro build --type quickapp",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:jd": "npm run build:jd -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "author": "",
  "dependencies": {
    "@babel/runtime": "^7.7.7",
    "@tarojs/cli": "3.4.10",
    "@tarojs/components": "3.4.10",
    "@tarojs/runtime": "3.4.10",
    "@tarojs/taro": "3.4.10",
    "@tarojs/react": "3.4.10",
    "@tarojs/plugin-framework-react": "3.4.10",
    "classnames": "^2.2.6",
    "reselect": "^4.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "taro-hooks": "latest",
    "react-redux": "^7.2.6",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "taro-ui": "^3.0.0-alpha.10",
    "axios": "^0.26.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@tarojs/mini-runner": "3.4.10",
    "@tarojs/webpack-runner": "3.4.10",
    "@types/webpack-env": "^1.13.6",
    "babel-plugin-import": "^1.13.3",
    "babel-preset-taro": "3.4.10",
    "eslint": "^8.12.0",
    "eslint-plugin-taro": "3.3.14",
    "eslint-plugin-typescript": "^0.12.0",
    "eslint-config-taro": "3.4.10",
    "stylelint": "9.3.0",
    "@typescript-eslint/parser": "^5.20.0",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "typescript": "^4.2.3",
    "@types/react": "^17.0.2",
    "eslint-plugin-react": "^7.8.2",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "@types/react-redux": "^7.1.20",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.4"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix --ext .js,.jsx,.ts,.tsx",
      "git add"
    ]
  }
}
